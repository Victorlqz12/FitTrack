@model IEnumerable<FitTrack.Models.Treino>

<h1>Treinos (Admin)</h1>

<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Data</th>
            <th>Nome</th>
            <th>UserId</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var t in Model)
        {
            <tr>
                <td>@t.Data.ToShortDateString()</td>
                <td>@t.NomeTreino</td>
                <td>@t.UserId</td>
            </tr>
        }
    </tbody>
</table>
