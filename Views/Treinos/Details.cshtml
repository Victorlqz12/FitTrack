@model FitTrack.Models.Treino

<h1>Detalhes do Treino</h1>

<p><strong>Data:</strong> @Model.Data.ToShortDateString()</p>
<p><strong>Nome do Treino:</strong> @Model.NomeTreino</p>
<p><strong>Usuário:</strong> @Model.Usuario?.Nome</p>

<a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id">Editar</a>
<a class="btn btn-secondary" asp-action="Index">Voltar</a>
